{
  "name": "@chatapp/frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 4225",
    "clean": "sh -c 'if [ -d dist ]; then chmod -R u+rwX dist || true; rm -rf dist; fi'",
    "prebuild": "npm run -s clean && node scripts/check-runtime-config.cjs",
    "build": "tsc -b && vite build",
    "build:alt": "tsc -b && VITE_OUTDIR=dist-new vite build",
    "preview": "vite preview --host 0.0.0.0 --port 4173",
    "lint": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "react-router-dom": "^6.26.2",
    "@tanstack/react-query": "^5.59.0",
    "keycloak-js": "^23.0.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@testing-library/react": "^14.1.2",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.21",
    "jsdom": "^24.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.4.5",
    "vite": "^5.4.10",
    "vitest": "^1.6.0"
  }
}
